<Window x:Class="CWCLog.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CWCLog"
        mc:Ignorable="d"
        Title="Log" Height="350" Width="525" Background="Black"
		Loaded="Window_Loaded">
    <Grid>
		<TextBox x:Name="TextBox_Log" Background="{x:Null}" Foreground="White" VerticalScrollBarVisibility="Visible"
				 AcceptsReturn="True" AcceptsTab="True" IsReadOnly="True"/>
	</Grid>
</Window>
